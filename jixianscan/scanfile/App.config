<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>


  <appSettings>
    <!--true显示Mac地址 false不显示-->
    <add key="showMacAddress" value="true"></add>
    <!--true打开文件  false打开文件夹-->
    <add key="IsOpenFile" value="false"></add>
    <add key="MacAddressGroup" value="omUOmJ3mOTaM0yUtbvmV3KEoXb+eLGnE"></add>
    <add key="postfix" value="JPG|TIF"></add>
    <add key="sourcePath" value="E:\极限测试\极限共享\"></add>
    <add key="targetPath" value="E:\极限测试\极限账单"></add>

    <!--以下内容不要修改-->
    <add key="noUnitReg" value="(?&lt;length&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*[ 乘X×-]+\s*(?&lt;width&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})"></add>

    <add key="bothUnitReg" value="(?&lt;length&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*(CM|[|M|米])+\s*[ 乘X×-]*\s*([长|宽|高|W|H]){0,1}(?&lt;width&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*(?&lt;Unit&gt;(CM|[|M|米])+)"></add>
    <add key="hasUnitAfterReg" value="(?&lt;length&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*(CM|[|M|米])*\s*[ 乘X×-]+\s*([长|宽|高|W|H]){0,1}(?&lt;width&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*(?&lt;Unit&gt;(CM|[|M|米])+)"></add>
    <add key="hasUnitBeforeReg" value="(?&lt;length&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*(?&lt;Unit&gt;(CM|[|M|米])+)\s*[ 乘X×-]+\s*([长|宽|高|W|H]){0,1}(?&lt;width&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*(CM|[|M|米])*[^0-9]+"></add>

    <add key="has3SplitReg" value="(?&lt;length&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})\s*[ X×-]+(?&lt;width&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})[ X×-]+(?&lt;flag&gt;\d{1,5}((\.|[点])\d{1,3}){0,1})[^\d张块份]"></add>
    <add key="qtyReg" value="(?&lt;QTY&gt;([一二两三四五六七八九十百千零]+|\d+))[张|块|份|次|个|条|面|幅]+"></add>
    <add key="floderReg" value="^\d{1,2}\.\d{1,2}$"></add>
  </appSettings>
</configuration>